{
  "name": "fetchmap",
  "version": "0.0.20",
  "description": "Non-throwing fetch wrapper",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "react-native": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "preversion": "npm run lint -- --fix && npm run build && npm run typecheck-test && npm t && npm run type-coverage && npm run typedoc",
    "postversion": "git add . && git commit -am \"lint & docs [skip build]\" || : && git push && git push --tags",
    "build": "rm -rf dist && rollup -c rollup.config.ts --configPlugin typescript && rm -rf tmp",
    "lint": "eslint .",
    "test": "jest",
    "typedoc": "typedoc src/index.ts && touch docs/.nojekyll",
    "typecheck-test": "tsc test/index.spec.ts --noEmit",
    "coverage": "codecov",
    "type-coverage": "type-coverage --detail --strict --ignore-files **/dist/**"
  },
  "keywords": [
    "fetch",
    "result"
  ],
  "files": [
    "dist",
    "src",
    "test"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/fetchmap/issues"
  },
  "homepage": "https://github.com/iyegoroff/fetchmap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/fetchmap.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "jest": "^27.5.1",
    "node-fetch": "^3.2.4",
    "prettier": "^2.6.2",
    "rollup": "2.73.0",
    "rollup-plugin-dts": "^4.2.1",
    "type-coverage": "^2.21.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/(?!(node-fetch|data-uri-to-buffer|fetch-blob|formdata-polyfill)/)"
    ]
  },
  "typeCoverage": {
    "atLeast": 99
  }
}
