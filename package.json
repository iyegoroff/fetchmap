{
  "name": "fetchmap",
  "version": "0.0.11",
  "description": "Non-throwing fetch wrapper",
  "sideEffects": false,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "preversion": "npm run lint -- --fix && npm run build && npm run typecheck-test && npm t && npm run type-coverage",
    "postversion": "git add . && git commit -am \"lint [skip build]\" || : && git push && git push --tags",
    "build:dts": "tsc -p configs/tsconfig.dts.json",
    "build:esm": "tsc -p configs/tsconfig.esm.json",
    "build:cjs": "build-for-cjs configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs",
    "lint": "eslint .",
    "watch": "tsc -p . -w",
    "test": "jest",
    "typecheck-test": "tsc test/index.spec.ts --noEmit",
    "coverage": "codecov",
    "type-coverage": "type-coverage --detail --strict --ignore-files **/dist/**"
  },
  "keywords": [
    "fetch",
    "result"
  ],
  "files": [
    "dist",
    "src",
    "test"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/fetchmap/issues"
  },
  "homepage": "https://github.com/iyegoroff/fetchmap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/fetchmap.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "build-for-cjs": "^0.0.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "express": "^4.18.1",
    "jest": "^27.5.1",
    "node-fetch": "^3.2.4",
    "prettier": "^2.6.2",
    "type-coverage": "^2.21.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/(?!(node-fetch|data-uri-to-buffer|fetch-blob|formdata-polyfill)/)"
    ],
    "moduleNameMapper": {
      "(.+)\\.js$": "$1"
    }
  },
  "typeCoverage": {
    "atLeast": 99
  }
}
